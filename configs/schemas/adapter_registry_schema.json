{
  "name": "adapter_registry",
  "document": {
    "name": "adapter_registry",
    "fields": [
      {
        "name": "adapter_id",
        "type": "string",
        "indexing": ["summary", "attribute"],
        "attribute": ["fast-search"]
      },
      {
        "name": "tenant_id",
        "type": "string",
        "indexing": ["summary", "attribute"],
        "attribute": ["fast-search"]
      },
      {
        "name": "name",
        "type": "string",
        "indexing": ["summary", "attribute"]
      },
      {
        "name": "version",
        "type": "string",
        "indexing": ["summary", "attribute"]
      },
      {
        "name": "base_model",
        "type": "string",
        "indexing": ["summary", "attribute"]
      },
      {
        "name": "model_type",
        "type": "string",
        "indexing": ["summary", "attribute"],
        "attribute": ["fast-search"]
      },
      {
        "name": "agent_type",
        "type": "string",
        "indexing": ["summary", "attribute"],
        "attribute": ["fast-search"]
      },
      {
        "name": "training_method",
        "type": "string",
        "indexing": ["summary", "attribute"]
      },
      {
        "name": "adapter_path",
        "type": "string",
        "indexing": ["summary"]
      },
      {
