      {
        "name": "training_config",
        "type": "string",
        "indexing": ["summary"]
      },
      {
        "name": "experiment_run_id",
        "type": "string",
        "indexing": ["summary", "attribute"]
      },
      {
        "name": "created_at",
        "type": "string",
        "indexing": ["summary", "attribute"]
      },
      {
        "name": "updated_at",
        "type": "string",
        "indexing": ["summary", "attribute"]
      }
    ]
  },
  "fieldsets": [
    {
      "name": "default",
      "fields": ["adapter_id", "tenant_id", "name", "agent_type", "status"]
