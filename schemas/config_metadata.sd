schema config_metadata {
    document config_metadata {
        field config_id type string {
            indexing: summary | attribute
            attribute: fast-search
        }

        field tenant_id type string {
            indexing: summary | attribute
            attribute: fast-search
        }

        field scope type string {
            indexing: summary | attribute
            attribute: fast-search
        }

        field service type string {
            indexing: summary | attribute
            attribute: fast-search
        }

        field config_key type string {
            indexing: summary | attribute
        }

        field config_value type string {
            indexing: summary
        }

        field version type int {
            indexing: summary | attribute
            attribute: fast-search
        }

        field created_at type string {
            indexing: summary | attribute
        }

        field updated_at type string {
            indexing: summary | attribute
        }
    }

    fieldset default {
        fields: config_id, tenant_id, scope, service, config_key
    }

    rank-profile default {
        first-phase {
            expression: attribute(version)
        }
    }
}
